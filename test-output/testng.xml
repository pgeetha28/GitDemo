<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AllDemoTests">

	<test name="DemoAnnotations">
		<classes>
			<class name="org.testng.AllAnnotations">
				<methods>
					<exclude name="Test.*">
					</exclude>
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="DemoTest">
		<classes>
			<class name="org.testng.DemoTest">
				<methods>
					<include name="DemoTest">
					</include>
				</methods>
			</class>
		</classes>
</test>
</suite> 
